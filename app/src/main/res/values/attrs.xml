<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="MyTextView">

    </attr>
    <declare-styleable name="TextView">
        <attr name="drawText" format="string" />
        <attr name="drawTextColor" format="color|reference" />
        <attr name="drawTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="QQStepView">
        <attr name="outerColor" format="color|reference" />
        <attr name="borderWidth" format="dimension" />
        <attr name="innerColor" format="color|reference" />
        <attr name="stepTextColor" format="color|reference" />
        <attr name="stepTextSize" format="dimension" />
    </declare-styleable>


    <declare-styleable name="ProgressBar">
        <attr name="innerBackground" format="color" />
        <attr name="outBackground" format="color" />
        <attr name="progressBorderWidth" format="dimension" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="progressTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RatingBar">
        <attr name="starNormal" format="reference" />
        <attr name="starSelect" format="reference" />
        <attr name="gradeNumber" format="integer" />
        <attr name="rating_padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LetterSideView">
        <attr name="sideTextSize" format="dimension" />
        <attr name="sideTextColor" format="color" />
    </declare-styleable>
</resources>